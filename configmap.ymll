apiVersion: v1
kind: ConfigMap
metadata:
  name: noted-backend-config
data:
  AWS_REGION: us-east-1
  ENV_NAME: production
  ALLOWED_HEADERS: |
    Accept
    Content-Type
    Content-Length
    Authorization
    X-Amz-date
    X-Api-Key
    X-Amz-Security-Token
    X-Amz-User-Agent
    app_user_id
    app_user_name
